<script>
$(document).ready(function () {
  $(".news-cont img.lazy").lazyload({ effect: "fadeIn" });
});

$(function () {
  const countryCodeUrl = 'https://www.honor.com/content/dam/honor/common/plugins/countryCode.json';

  const URL = window.location.href;
  let currentCountry = '';
  const urlParts = URL.split('/');
  if (urlParts.length >= 4) {
    currentCountry = urlParts[3];
  }

  // 获取 countryCode.json 数据
  fetch(countryCodeUrl)
    .then(response => response.json())
    .then(countryData => {
      // 获取当前页面的 expectedCountry
      return fetch(URL)
        .then(response => {
          if (response.ok) {
            let expectedCountry = response.headers.get('expected-country');
            if (expectedCountry && currentCountry) {
              if (expectedCountry !== currentCountry) {
                let currentSite = localStorage.getItem('currentSite');
                if (currentSite !== 'true') {
                  // 通过对比 JSON 中的 countryCode 是否匹配 expectedCountry，得到 country
                  const matchedCountry = countryData.find(item => 
                    item.countryCode === expectedCountry
                  );
                  
                  const recommendText = matchedCountry 
                    ? `${matchedCountry.country}`
                    : 'Recommend Site';
                  
                  $('.top-country-select .btn-current').before(
                    `<a class="btn-expected" href="https://www.honor.com/${expectedCountry}/">${recommendText}</a>`
                  );
                  
                  $('body').on('click', '.top-country-select .btn-expected', function (e) {
                    e.preventDefault();
                    localStorage.setItem('currentSite', 'true');
                    window.location.href = $(this).attr('href');
                  });
                  $('body').addClass('country-select-show');
                }
              } else {
                $('body').addClass('country-select-hide');
              }
            }
          } else {
            console.log('Error: ', response.status);
          }
        });
    })
    .catch(error => console.error('Fetch error:', error));

  $('body').on('click', '.top-country-select .btn-current,.top-country-select .btn-close', function () {
    $('body').removeClass('country-select-show');
  });
})
</script>



